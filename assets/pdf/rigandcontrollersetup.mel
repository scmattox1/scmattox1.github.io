joint -p 33.250819 16.989276 -0.332901 ;
joint -e -zso -oj xyz -sao yup joint12;
joint -p 34.281914 17.00875 -0.264551 ;
joint -e -zso -oj xyz -sao yup joint13;
joint -p 34.95428 16.996302 -0.234322 ;
joint -e -zso -oj xyz -sao yup joint14;
//Middle
select -cl  ;
select -r joint6;
joint -p 31.901656 16.998959 -1.138157 ;
joint -e -zso -oj xyz -sao yup joint6;
joint -p 33.17073 16.986975 -1.1168 ;
joint -e -zso -oj xyz -sao yup joint16;
joint -p 34.201825 17.014834 -1.048451 ;
joint -e -zso -oj xyz -sao yup joint17;
joint -p 34.874191 16.994001 -1.018222 ;
joint -e -zso -oj xyz -sao yup joint18;
//Ring
select -cl  ;
select -r joint6;
joint -p 31.832144 17.011195 -1.818522 ;
joint -e -zso -oj xyz -sao yup joint6;
joint -p 33.101219 16.988458 -1.797166 ;
joint -e -zso -oj xyz -sao yup joint20;
joint -p 34.132313 17.007932 -1.728816 ;
joint -e -zso -oj xyz -sao yup joint21;
joint -p 34.80468 16.995485 -1.698588 ;
joint -e -zso -oj xyz -sao yup joint22;
//Pinky
select -cl  ;
select -r joint6;
joint -p 31.764144 17.008455 -2.484098 ;
joint -e -zso -oj xyz -sao yup joint6;
joint -p 33.033219 16.962933 -2.462742 ;
joint -e -zso -oj xyz -sao yup joint24;
joint -p 34.064313 17.007561 -2.394392 ;
