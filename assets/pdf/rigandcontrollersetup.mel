/*
/Sarah Mattox's Maya Auto Control Rig      
/www.scmattox.weebly.com/rig
/
/This script creates a rig and controller system.
/
/To Use:
/Run the script. After clicking first button, adjust joints to /fit model. After clicking second button, scale controllers to /fit model. Third button cleans up and hides unused attributes.
/
/Noted Features:
/FK/IK legs and arms
/IK legs and arms can follow body motion
/Hands and Feet custom attributes
*/


if (`window -exists myWindow`) deleteUI myWindow;
// create a window
window -title "Rig and Controller Setup" myWindow ;
 formLayout -numberOfDivisions 100 myForm; 
// define the layout of controls added to the window.
//columnLayout -bgc 12 90 166;

// create a couple of buttons
button -bgc 255 195 0 -label "Left Side Joint Setup" -command "setup_func" myBtn1;
text -label "Translate joints to fit model" myTxt1;

button -bgc 255 83 0 -label "Mirror Joints & Add Controllers" -command "mirror_func" myBtn2;
text -label "Rig Complete. IKFK arms and legs. FK back." myTxt2;
text -label "Driven Keys for hands and feet." myTxt3;
text -label "World/Local Switch added to IK arms and legs." myTxt4;

button -bgc 255 195 0 -label "Clean Up" -command "extra_func" myBtn3;
text -label "Locked unavailble attributes in controllers." myTxt5;

formLayout -edit 
 -attachForm myBtn1 "top" 30 
 -attachForm myBtn1 "left" 110
 -attachForm myTxt1 "top" 60
 -attachForm myTxt1 "left" 100

 -attachForm myBtn2 "top" 100
 -attachForm myBtn2 "left" 90 
-attachForm myTxt2 "top" 130
 -attachForm myTxt2 "left" 60 

 -attachForm myTxt3 "top" 145
 -attachForm myTxt3 "left" 85 

 -attachForm myTxt4 "top" 160
 -attachForm myTxt4 "left" 55 

 -attachForm myBtn3 "top" 200
 -attachForm myBtn3 "left" 140  

 -attachForm myTxt5 "top" 230
 -attachForm myTxt5 "left" 60
myForm;  

// show the window we last created
showWindow;

proc setup_func() {
