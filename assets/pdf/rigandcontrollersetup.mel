joint -p 0 -3.722654 -0.0581521 ;
joint -p 0 -21.104744 1.871562 ;
joint -e -zso -oj xyz -sao yup joint12;
joint -p 0 -37.06791 -2.385282 ;
joint -e -zso -oj xyz -sao yup joint13;
joint -p 0 -40.041787 0.988412 ;
joint -e -zso -oj xyz -sao yup joint14;
joint -p 0 -40.041787 5.554677 ;
joint -e -zso -oj xyz -sao yup joint15;
select -r joint12 ;
move -r 5.043682 0 0 ;
select -tgl joint1 ;
parent;

//Naming Main Joints and Leg
rename "joint1" "Pelvis";
rename "joint2" "Lumbar1";
rename "joint3" "Lumbar2";
rename "joint4" "Thorax1";
rename "joint5" "Thorax2";
rename "joint6" "Scapula";
rename "joint7" "Neck1";
rename "joint8" "Neck2";
rename "joint9" "Neck3";
rename "joint10" "Head_Center";
rename "joint11" "Head_Top";
rename "joint12" "L_Hip";
rename "joint13" "L_Knee";
rename "joint14" "L_Ankle";
rename "joint15" "L_Ball";
rename "joint16" "L_Toe";

//Jaw Joints
select -d;
joint -p 0 22.971913 1.514827 ;
joint -p 0 21.633058 4.273681 ;
joint -e -zso -oj xyz -sao yup joint1;

//Naming Jaw Joints
rename "joint1" "Jaw";
