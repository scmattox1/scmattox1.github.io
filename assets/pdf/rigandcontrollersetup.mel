//Left Arm Joints
//Collar, Clavicle
select -cl  ;
// not needed, will deleter later
joint -p -20.699941 0 -44.383622 ;
joint -e -zso -oj xyz -sao yup joint1;
select -cl  ; 
joint -p 2.439165 17.003714 -0.0151729 ;
joint -e -zso -oj xyz -sao yup joint2;
joint -p 7.439165 17.003714 -0.991568 ;
joint -e -zso -oj xyz -sao yup joint3;

//Arm, Hand, Thumb
joint -p 18.835239 16.993885 -2.007224 ;
joint -e -zso -oj xyz -sao yup joint4;
joint -p 29.424385 17.007733 -1.020303 ;
joint -e -zso -oj xyz -sao yup joint5;
joint -p 30.073172 17.011195 -0.8994 ;
joint -e -zso -oj xyz -sao yup joint6;
joint -p 30.787572 17.011195 0.0866132 ;
joint -e -zso -oj xyz -sao yup joint7;
joint -p 31.49164 16.975895 0.663485 ;
joint -e -zso -oj xyz -sao yup joint8;
joint -p 31.997327 16.953533 1.095931 ;
joint -e -zso -oj xyz -sao yup joint9;
joint -p 32.610584 16.932135 1.577177 ;
joint -e -zso -oj xyz -sao yup joint10;
select -cl  ;

//Fingers
//Index
select -cl  ;
select -r joint6;
